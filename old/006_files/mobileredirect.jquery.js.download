(function( $ ){
	$(function(){
		$(".bypassing").hide();
	});
    var mobileRedirect = {
		init: function( options ) {  
			// Create some defaults, extending them with any options that were provided
			var settings = $.extend( {
			  'redirectTo': ''
			}, options);
            
            if (settings.redirectTo == '')
            {
                console.log("You must specify a link to redirect to.")
                return;
            }
            
			function isMobile() {
                return $.browser.mobile;
            }
            function isBypassing() {
                if ((isMobile() == true) && ($.cookies.get('bypass_mobile') == true))
                {
                    return true;    
                }
                return false;
            }
           
            if (isMobile()) {
                var $_GET = jQuery.queryParams();
                if ($_GET['nomobile'] == 'false')
                {
                    $.cookies.del("bypass_mobile")
                }
                else if ($_GET['nomobile'] == 'true')
                {
                    $.cookies.set("bypass_mobile",true)
                }
                
                if (isBypassing() == false) {
                    window.location = settings.redirectTo;
                }
				else
				{
					$(function(){$(".bypassing").show();})
				}
            }
		}
	}
    
	$.extend({
		'mobileRedirect': mobileRedirect
	});
    
    $.extend({
        queryParams: function() {
            var qs = document.location.search;
            qs = qs.split("+").join(" ");
            var params = {},
                tokens,
                re = /[?&]?([^=]+)=([^&]*)/g;
        
            while (tokens = re.exec(qs)) {
                params[decodeURIComponent(tokens[1])]
                    = decodeURIComponent(tokens[2]);
            }
        
            return params;
        } 
    });
})( jQuery );